{
  "manifest_version": 2,
  "name": "Script Injector",
  "version": "1.0.0",
  "description": "Inject custom scripts using hotkeys",
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs",
    "activeTab"
  ],
  "icons": {
    "16": "./icons/icon-16.png",
    "48": "./icons/icon-48.png",
    "128": "./icons/icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
