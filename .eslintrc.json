{
  "globals": {
    "browser": "readonly"
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["solid"],
  "extends": ["eslint:recommended", "plugin:solid/recommended"],
  "rules": {
    "@typescript-eslint/no-non-null-assertion": "off"
  }
}
